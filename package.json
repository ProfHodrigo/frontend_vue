{
  "name": "frontend-vue-curso",
  "version": "1.0.0",
  "description": "Curso de Vue.js - Frontend consumindo API Flask",
  "main": "src/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "lint": "eslint src --ext .vue,.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:aws": "node scripts/deploy-aws.js",
    "docker:build": "docker build -t vue-app:latest .",
    "docker:run": "docker run -p 80:80 vue-app:latest"
  },
  "dependencies": {
    "vue": "^3.3.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.0",
    "vite": "^4.3.0"
  },
  "keywords": [
    "vue",
    "javascript",
    "frontend",
    "api",
    "flask",
    "curso",
    "educacional"
  ],
  "author": "Prof. Rodrigo",
  "license": "MIT"
}